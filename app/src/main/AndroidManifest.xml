<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.felipe.nossafeira" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/carrinho_ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/carrinho_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NossaFeira" >
        <activity
            android:name=".activities.AddEditItemActivity"
            android:exported="false" />
        <activity
            android:name=".activities.ListItemsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.ListManagingActivity"
            android:exported="false" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>